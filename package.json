    "@capacitor/ios": "~4.6.2",
    "@capacitor/splash-screen": "~4.1.3",
    "@ionic-enterprise/identity-vault": "~5.9.5",
    "@ionic-enterprise/secure-storage": "~2.3.4",
    "@mdi/font": "~7.1.96",
    "@vitest/coverage-c8": "~0.27.2",
    "@vue/test-utils": "~2.2.7",
    "axios": "~1.2.3",
    "jsdom": "~21.1.0",
    "roboto-fontface": "*",
    "vee-validate": "~4.7.3",
    "vitest": "~0.27.2",
    "vue": "^3.2.38",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0",
    "webfontloader": "^1.0.0",
    "yup": "~0.32.11"
