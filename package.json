{
  "name": "security-trifecta-vuetify",
  "version": "0.0.0",
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#fafafa' --splashBackgroundColor '#fafafa' --iconBackgroundColorDark '#646464' --splashBackgroundColorDark '#646464'",
    "build": "vue-tsc --noEmit && vite build && cap sync",
