    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "~4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "~8.0.3",
    "prettier": "~2.8.3",
    "pretty-quick": "~3.1.3",
    "typescript": "^4.0.0",
    "vite": "~4.0.4",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-tsc": "^1.0.9",
    "wait-for-expect": "~3.0.2"
